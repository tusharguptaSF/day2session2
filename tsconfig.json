{
    "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "lib": [
        "ES2015",
        "dom"
     ],
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "out",
      "sourceMap": true
    },
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended"
  }